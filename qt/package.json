{
  "name": "qt",
  "version": "1.55.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode develop",
    "build": "vue-cli-service build  --mode develop",
    "deploy": "vue-cli-service build --mode develop; firebase deploy --only hosting:develop-qt-app -P develop",
    "live:serve": "vue-cli-service serve --mode live",
    "develop:build": "vue-cli-service build --mode develop",
    "live:build": "vue-cli-service build --mode live",
    "develop:deploy": "vue-cli-service build --mode develop; firebase deploy --only hosting:develop-qt-app -P develop",
    "live:deploy": "vue-cli-service build --mode live; firebase deploy --only hosting:live-qt-app -P live",
    "deploy-service-offline:develop": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:develop-qt-app -P develop",
    "deploy-service-offline:live": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:live-qt-app -P live",
    "lint": "vue-cli-service lint",
    "lint-ci": "vue-cli-service lint --no-fix",
    "test": "TZ='Europe/London' vue-cli-service test:unit",
    "test:coverage": "TZ='Europe/London' vue-cli-service test:unit --coverage",
    "test-ci": "TZ='Europe/London' vue-cli-service test:unit --ci --runInBand"
  },
  "dependencies": {
